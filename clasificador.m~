function [] = clasificador()





figuras = {'caracols'
    'castillos'
    'conejos'
    'corazons'
    'geckos'
    'globos'
    'huesos'
    'libelulas'
    'lunas'
    'manchas'
    'mariposas'
    'ninas'
    'ninos'
    'osos'
    'pastels'
    'tulipans'};



for i=1:size(figuras,1)
    
   path = [figuras{i} '.mat']
   
   
   
   
    
    
end




end